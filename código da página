            <fieldset>
              <legend>Como prefere nosso contato?</legend>
              <label for="email-radio"><input type="radio" name="contato" value="email" id="email-radio">Email</label>
              
              <label for="radio-telefone"><input type="radio" name="contato" value="telefone" id="radio-telefone">Telefone</label>

              <label for="Whatsapp-radio"><input type="radio" name="contato" value="mensagem" id="Whatsapp-radio" checked>Whatsapp</label>
            </fieldset>

